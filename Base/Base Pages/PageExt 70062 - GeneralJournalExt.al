    [LineStart(2243)]
    procedure CommaRemoval(Base : Text[250]) Converted : Text[250];
    var
        i : Integer;
    begin
         FOR i:=1 TO STRLEN(Base) DO
         BEGIN
           IF (COPYSTR(Base,i,1)<>',') AND (COPYSTR(Base,i,1)<>'''') AND (COPYSTR(Base,i,1)<>'&') THEN
             Converted+=COPYSTR(Base,i,1);
         END;
         EXIT(Converted);
    end;

